# ~/.mrxvtrc - Config file of mrxvt
# $Header: /opt/cvsroot/conf/.mrxvtrc,v 1.2 2006/06/01 07:00:27 jick Exp $

mrxvt*termName:		xterm    
mrxvt*geometry:		80x25
mrxvt*saveLines:	1000
mrxvt*foreground:	white
mrxvt*background:	black

mrxvt*mfont:		hanzigb16st
mrxvt*xftmfont:		SimSun
#mrxvt*font":	-misc-simsun-medium-r-normal--0-0-0-0-c-0-gb2312.1980-0
mrxvt*font":		Tahoma
mrxvt*xftSize:            14
#mrxvt*xft:True
#mrxvt*xftFont:Luxi Mono
#mrxvt*xftmFont:SimSun
#mrxvt*xftNomFont:True
#mrxvt*xftAntialias:True
#mrxvt*xftWeight:Medium
#mrxvt*xftSlant:Roman
#mrxvt*xftHinting:True
#mrxvt*xftAutoHint:True
#mrxvt*xftGlobalAdvance:True
#mrxvt*xftWidth:normal
#mrxvt*xftRGBA:rgb

mrxvt*multichar_encoding: GB
mrxvt*inputMethod:        fcitx

#mrxvt*vt0*Pixmap:      /usr/share/wallpapers/Paper01.jpg

mrxvt*scrollTtyOutputInhibit:     True
mrxvt*scrollWithBuffer:            True

